                                            // Ouverture du popup 
                                            var fen = window.open("", "", "height=800, width=600,toolbar=0, menubar=0, scrollbars=1, resizable=1,status=0, location=0, left=10, top=10");
                                            // style du popup 
                                            fen.document.body.style.color = '#000000';
